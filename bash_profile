#!/bin/bash
#: Title        : bash_profile
#: Description  : 2011-09-11
#: Author       : "Marc Jacobs" <marcja@gmail.com>
#: Version      : 1.0
#: Description  : Personal profile for bash login shells. We recommend that you
#                 make sure this sources ~/.bashrc, then ignore it.
#: Options      : None

echo info: sourcing $HOME/.bash/bash_profile

[ -f $HOME/.bashrc ] && source $HOME/.bashrc
